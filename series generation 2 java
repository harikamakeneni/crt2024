import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner s=new Scanner(System.in);
        int n;
        n=s.nextInt();
        int i=1;
        int r1=7,r2=5;
        int x=n;
        n=n/2;
        while(i<=n){
        i++;
        System.out.printf("%d ",r1);
            r1++;
            System.out.printf("%d ",r2);
            r2++;
        }
        if(x%2!=0)
            System.out.printf("%d ",r1);
    }
}
